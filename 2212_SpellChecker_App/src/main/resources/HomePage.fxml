<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainPage" maxHeight="676.0" maxWidth="1032.0" minHeight="676.0" minWidth="1032.0" prefHeight="676.0"
            prefWidth="1032.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="SpellCheckerApp.HomePageController">
   <Text id="titleText" fx:id="titleText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Spell Check"
         x="401.0" y="129.0">
      <font>
         <Font size="40.0"/>
      </font>
   </Text>
   <Text id="nameText" fx:id="nameText" fill="WHITE" layoutX="-95.0" layoutY="31.0" strokeType="OUTSIDE"
         strokeWidth="0.0" text="By: Theo Cho, Ameen Al-Asady, Alana Johannson, Abeer Muhammad, Sajeiv Ravichandran"
         textAlignment="CENTER" x="233.0" y="200.0"/>
   <Button id="buttonMain" fx:id="openFileButton" alignment="CENTER" contentDisplay="TOP" layoutX="222.0"
           layoutY="311.0" mnemonicParsing="false" onAction="#handleOpenFile" prefHeight="56.0" prefWidth="587.0"
           stylesheets="@style.css" text="Upload A Text Document (.txt)" textAlignment="CENTER" textFill="WHITE"
           textOverrun="CLIP">
      <padding>
         <Insets bottom="10.0" left="16.0" right="16.0" top="10.0"/>
      </padding>
      <font>
         <Font name="Perpetua Bold" size="24.0"/>
      </font>
   </Button>
   <Button id="buttonMain" fx:id="buttonMain" layoutX="300.0" layoutY="431.0" mnemonicParsing="false"
           onAction="#openUserDict" prefHeight="56.0" prefWidth="431.0" text="Edit User Dictionary" textFill="WHITE">
      <font>
         <Font name="Perpetua Bold" size="24.0"/>
      </font>
   </Button>
   <Button id="buttonMain" fx:id="exitButton" layoutX="893.0" layoutY="36.0" mnemonicParsing="false" onAction="#exitProgram"
           prefWidth="100.0" text="Exit" textAlignment="CENTER" textFill="WHITE">
      <font>
         <Font name="Perpetua Bold" size="24.0"/>
      </font>
   </Button>
</AnchorPane>
